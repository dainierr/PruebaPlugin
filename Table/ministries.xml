<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>name</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <column>
        <name>notes</name>
        <type>text</type>
    </column>

   
   <!--constraint-->
    <constraint>
        <name>events_pkey</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>

</table>